<tr>
  <td class="tbl_sm text-center"><%= link_to quote.symbol, quote %> <br><small><%= current_user.get_symbol_qty(quote.symbol) %></small> </td>
  <td class="tbl_sm text-center"><%= quote.exchange %></td>
  <td class="tbl_md"><%= quote.name %></td>
  <td class="text-center"><%= to_currency quote.latest_price, precision: 2 %></td>
  <td class="text-center"><%= to_currency quote.prev_close, precision: 2 %></td>
  <td class="text-center"><%= quote.change %></td>
  <td class="text-center"><%= quote.change_percent_s %></td>
  <td class="text-center"><%= quote.volume %></td>
  <td class="text-center"><%= to_currency quote.week52high, precision: 2 %></td>
  <td class="text-center"><%= to_currency quote.week52low, precision: 2 %></td>
  <td class="text-center"><%= quote.ytd_change_str %>%</td>
  <td class="tbl_md text-center"><%= quote.latest_update %></td>
  <td class="text-center"><%= link_to '<i class="bi-arrow-repeat"></i>'.html_safe, quote, method: :patch  %> </td>
  <td class="text-center"><%= link_to '<i class="bi-trash"></i>'.html_safe, quote, method: :delete, data: { confirm: "Are you sure you want to delete quote for #{quote.symbol}?" } %> </td>
</tr>

